let ten = 5 + 5
printfn "5 + 5 = %d" ten

let four = 8 - 4
printfn "8 - 4 = %d" four

let five = 10 / 2
printfn "10 / 2 = %d" five

let twenty = 10 * 2
printfn "10 * 2 = %d" twenty

let remainder = 10 % 2
printfn "10 %% 2 = %d" remainder

let squared = 10.0 ** 2.0
printfn "10.0 ** 2.0 = %f" squared
